<?xml version="1.0" encoding="utf-8"?>
<set
  xmlns:android="http://schemas.android.com/apk/res/android">
    <objectAnimator android:duration="100" android:valueFrom="0" android:valueTo="1.0" android:propertyName="alpha" />
    <objectAnimator android:duration="300" android:valueFrom="1.0" android:valueTo="1.2" android:propertyName="scaleX" />
    <objectAnimator android:duration="300" android:valueFrom="1.0" android:valueTo="1.2" android:propertyName="scaleY" />
    <objectAnimator android:duration="150" android:startOffset="300" android:valueFrom="1.2" android:valueTo="1.1" android:propertyName="scaleX" />
    <objectAnimator android:duration="150" android:startOffset="300" android:valueFrom="1.2" android:valueTo="1.1" android:propertyName="scaleY" />
    <objectAnimator android:interpolator="@android:interpolator/accelerate_decelerate" android:duration="600" android:startOffset="100" android:valueFrom="0" android:valueTo="-90" android:propertyName="rotationY" />
</set>